<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.niku.moneymate">

    <application
        android:name=".MoneyMateApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_action_navbar_item_account"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_action_navbar_item_account"
        android:supportsRtl="true"
        android:theme="@style/Theme.Moneymate">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>